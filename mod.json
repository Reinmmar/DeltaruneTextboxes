{
    "geode": "3.3.1",
    "gd": {
        "win": "2.206",
        "android": "2.206",
        "mac": "2.206",
        "ios": "2.206"
    },
    "id": "timestepyt.deltarune_textboxes",
    "name": "Deltarune Textboxes",
    "version": "v0.2.2",
    "developer": "TimeStepYT",
    "description": "Makes FLAlertLayers look like textboxes from Deltarune",
    "repository": "https://github.com/Reinmmar/DeltaruneTextboxes",
    "tags": ["customization"],
    "dependencies": [
        {
            "id": "geode.node-ids",
            "importance": "required",
            "version": ">=1.10.0"
        }
    ],
    "resources": {
        "fonts": {
            "Determination": {
                "path": "resources/determination-extended.ttf",
                "size": 78
            }
        },
        "sprites": ["resources/deltaruneSquare.png", "resources/heart.png"]
    },
    "settings": {
        "dontRestrictWidth": {
            "name": "Don't limit width",
            "type": "bool",
            "default": false,
            "description": "Doesn't limit the <cy>textbox width</c> to max width for <cj>16:9</c>"
        },
        "disableClickToProgress": {
            "name": "Don't progress by clicking",
            "type": "bool",
            "default": false,
            "description": "Disables <cy>clicking</c> to progress textboxes",
            "platforms": ["win", "macos"]
        }
    }
}
